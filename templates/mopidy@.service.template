[Unit]
Description=Mopidy music server
After=avahi-daemon.service
After=dbus.service
After=network-online.target
Wants=network-online.target
After=nss-lookup.target
After=pulseaudio.service
After=remote-fs.target
After=sound.target

[Service]
#User=mopidy
ExecStart=/home/gnome/.venvs/mopidy/bin/python /usr/bin/mopidy --config /usr/share/mopidy/conf.d:/home/gnome/network/multizone-audio/etc/multizone-audio/mopidy.%i.conf

[Install]
WantedBy=multi-user.target
